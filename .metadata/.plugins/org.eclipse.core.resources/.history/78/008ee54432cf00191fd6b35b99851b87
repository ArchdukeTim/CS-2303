#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int nRow = 3;
	int nCol = 3;
	int board[nRow][nCol];
	for (int i = 0; i < nRow; i++) {
		for (int j = 0; j < nCol; j++) {
			board[i][j] = 0;
			printf("%d", board[i][j]);
		}
	}
}
