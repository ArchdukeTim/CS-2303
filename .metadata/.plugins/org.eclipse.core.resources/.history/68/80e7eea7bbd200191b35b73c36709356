#ifndef BOARD_H_
#define BOARD_H_
#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#include <stdbool.h>

#define ROWS 21
#define COLS 21
#define EDGE "%"

typedef struct{
	int array[ROWS][COLS];
	int x;
	int y;
} Board;

bool tests();

Board makeBlankBoard();

#endif /* BOARD_H_ */
