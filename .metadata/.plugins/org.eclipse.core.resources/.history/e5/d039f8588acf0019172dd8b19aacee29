#ifndef PRODUCTION_H_
#define PRODUCTION_H_
#include <stdio.h>
#include <stdbool.h>


typedef struct{
	int x;
	int y;
} Location;

#define nROW 10
#define nCol 10

typedef struct{
	int array[nROW][nCol];
	Location point;
} Matrix;

bool tests();

Matrix makeBlankMatrix();

void printArray(int *array);

bool production(int argc, char* argv[]);

#endif /* PRODUCTION_H_ */
