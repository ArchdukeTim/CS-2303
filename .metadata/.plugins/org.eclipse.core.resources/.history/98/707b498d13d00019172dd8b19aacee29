#ifndef PRODUCTION_H_
#define PRODUCTION_H_
#include <stdio.h>
#include <stdbool.h>

#define ROWS 10
#define COLS 10

typedef struct{
	int x;
	int y;
} Location;


typedef struct{
	int array[ROWS][COLS];
	Location point;
} Matrix;

bool tests();

Matrix makeBlankMatrix();

void printMatrix(int *array);

bool production(int argc, char* argv[]);

#endif /* PRODUCTION_H_ */
