#include "production.h"

int makeBlankArray(int nRow, int nCol) {
	int board[nRow][nCol];
	for (int i = 0; i < nRow; i++) {
		for (int j = 0; j < nCol; j++) {
			board[i][j] = 0;
		}
	}
	return board
}

bool production(int argc, char *argv[]) {
	return false;
}
